<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caotiancheng.pwdnote">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fufu"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fufu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".loginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".listActivity" android:label="ROMA" android:logo="@drawable/nero"></activity>
        <activity android:name=".resetActivity"></activity>
        <activity android:name=".addActivity" android:label="Add UMU" android:logo="@drawable/nero"></activity>
        <activity android:name=".editActivity" android:label="Edit UMU" android:logo="@drawable/nero"></activity>
        <activity android:name=".deleteActivity" android:label="Delete UMU" android:logo="@drawable/nero"></activity>
    </application>

</manifest>